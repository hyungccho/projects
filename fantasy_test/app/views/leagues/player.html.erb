<div class="container">
  <div class="row">
    <div class="col-xs-4 col-xs-offset-1">
      <h2 class="cool-font"><%= @user.username %></h1>
      <img class="img-rounded" src="<%= image_url("default_pic.jpg") %>">
    </div>

    <div class="col-xs-4 col-xs-offset-1">
      <h3 class="cool-font">Drafted Players</h3>

      <ul>
        <% @players.each do |player| %>
          <li><%= player.lname %>, <%= player.fname %></li>
        <% end %>
      </ul>
    </div>
  </div>
</div>
